{
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "no-undef": 0,
        "no-unused-vars": 0,
        "react/prop-types": 0,
        "react/display-name": 0,
        "no-async-promise-executor": 0,
        "no-control-regex": 0
    }
}